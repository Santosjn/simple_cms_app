<% @page_title = "Edit Admin User #{@admin_user.name}"  %>

<%= link_to("Back to Admin Users List", {action: 'index'}, :class => 'back-link') %>

<div class="admin_users_edit">
	<h2>Update Admin User</h2>

	<%= form_for(:admin_user, url: {action: 'update', id: @admin_user.id}) do |f| %>
		
		<%= render(partial: "form", locals: {f: f}) %>

	<div class="form-buttons">
		<%= submit_tag('Update AdminUser') %>
	</div>

	<% end %>

</div>